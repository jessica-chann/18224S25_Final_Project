LOCATE COMP "sel[0]" SITE "U1"; # classic
IOBUF  PORT "sel[0]" PULLMODE=DOWN IO_TYPE=LVCMOS33;

LOCATE COMP "sel[1]" SITE "H16"; # timed
IOBUF  PORT "sel[1]" PULLMODE=DOWN IO_TYPE=LVCMOS33;

LOCATE COMP "sel[2]" SITE "R18"; # speed
IOBUF  PORT "sel[2]" PULLMODE=DOWN IO_TYPE=LVCMOS33;

LOCATE COMP "sel[3]" SITE "V1"; # reverse
